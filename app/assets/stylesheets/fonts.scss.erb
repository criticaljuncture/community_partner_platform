@font-face {
  font-family: 'community_partner_platform';
  src:url('<%= asset_path("community_partner_platform.eot") %>');
  src:url('<%= asset_path("community_partner_platform.eot") %> + "?#iefix"') format('embedded-opentype'),
          url('<%= asset_path("community_partner_platform.woff") %>') format('woff'),
          url('<%= asset_path("community_partner_platform.ttf") %>') format('truetype'),
          url('<%= asset_path("community_partner_platform.svg") %> + "#community_partner_platform"') format('svg');
  font-weight: normal;
  font-style: normal;
}

[class^="icon-cpp-"], [class*=" icon-cpp-"] {
	font-family: 'community_partner_platform';
	speak: none;
	font-style: normal;
	font-weight: normal;
	font-variant: normal;
	text-transform: none;
	line-height: 1;

	/* Better Font Rendering =========== */
	-webkit-font-smoothing: antialiased;
	-moz-osx-font-smoothing: grayscale;
}

.icon-cpp-help:before {
	content: "\e608";
}
.icon-cpp-close-x-inv:before {
	content: "\e607";
}
.icon-cpp-users:before {
	content: "\e606";
}
.icon-cpp-small-business:before {
	content: "\e600";
}
.icon-cpp-school:before {
	content: "\e601";
}
.icon-cpp-handshake:before {
	content: "\e602";
}
.icon-cpp-graph:before {
	content: "\e603";
}
.icon-cpp-gears:before {
	content: "\e604";
}
.icon-cpp-close-x:before {
	content: "\e605";
}
