<div class="community-programs-map-popup">
  <h1>
    <a href="{{schoolUrl}}">{{schoolName}}</a>
  </h1>
  <dl>
    <dt>Total Program Count</dt>
    <dd class="total-count">{{programCount}}</dd>
    {{#each schoolProgramsByElement}}
      <dt>{{this.name}}</dt>
      <dd>{{this.count}}</dd>
    {{/each}}
  </dl>
</div>
