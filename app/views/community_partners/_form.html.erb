<%= f.inputs do %>
  <%= f.input :organization_id, :as => :select, :collection => Organization.all %>
  <%= f.input :school_id, :as => :select, :collection => School.all %>
  <%= f.input :school_quality_indicator_sub_area_id, :as => :select, :collection => SchoolQualityIndicatorSubArea.all %>
  <%= f.input :service_type_id, :as => :select, :collection => ServiceType.all %>

  <%= f.input :notes %>
<% end %>

<%= f.actions do %>
  <%= f.button 'Save', class: "btn btn-success" %>
<% end %>
