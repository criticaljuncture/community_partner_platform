<%= breadcrumb [['Community Partnerships', community_partners_path()]] %>

<div class="row-fluid">
  <div class="span12">
    <div class="header">
      <h1>OUSD Community Partnerships</h1>
      <%= link_to 'Create New Partnership', new_community_partner_path(), :class => "btn btn-primary" %>
    </div>

    <%= page_nav_tabs [['Overview', '#overview'],
                       ['Community Partnerships', '#community-partners-table']] %>

    <div class="tab-content">
      <div id="overview" class="tab-pane active">
      </div>

      <div id="community-partners-table" class="tab-pane">
        <%= render partial: 'table', locals: {community_partners: @community_partners} %>
      </div>
    </div>
  </div>
</div>
