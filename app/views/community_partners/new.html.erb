<%= breadcrumb [
  ['Community Partnerships', community_partners_path()],
  ['Create', ""]
] %>

<div class="row-fluid">
  <div class="span12">
    <%= page_header('Create New Community Partnership', "icon-handshake") %>

    <div class="form_wrapper">
      <%= semantic_form_for @community_partner, method: :post, html: {class: "form-horizontal"} do |f| %>
        <%= render :partial => 'form', :locals => {:f => f} %>
      <% end %>
    </div>
  </div>
</div>

