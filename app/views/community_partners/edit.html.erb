<%= breadcrumb [['Community Partnerships', community_partners_path()], ['Community Partnership', community_partner_path(@community_partner)], ['Edit Community Partnership', ""]] %>

<div class="row-fluid">
  <div class="span12">
    <div class="header">
      <h1>Edit Community Partnership</h1>
    </div>

    <div class="form_wrapper">
      <%= semantic_form_for @community_partner, method: :put, html: {class: "form-horizontal"} do |f| %>
        <%= render :partial => 'form', :locals => {:f => f} %>
      <% end %>
    </div>

  </div>
</div>
