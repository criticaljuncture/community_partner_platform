<%= breadcrumb [['Service Types', service_types_path], ["New Service Type", ""]] %>

<div class="row-fluid">
  <div class="span12">
    <div class="header">
      <h1>New Service Type</h1>
    </div>

    <div class="form_wrapper">
      <%= semantic_form_for @service_type, method: :post, html: {class: "form-horizontal"} do |f| %>
        <%= render :partial => 'form', :locals => {:f => f} %>
      <% end %>
    </div>
  </div>
</div>

