<%= breadcrumb [['Service Types', service_types_path], [@service_type.name, service_type_path(@service_type)], ["Edit Service Type", ""]] %>

<div class="row-fluid">
  <div class="span12">
    <div class="header">
      <h1><%= @service_type.name %></h1>
    </div>

    <div class="form_wrapper">
      <%= semantic_form_for @service_type, method: :put, html: {class: "form-horizontal"} do |f| %>
        <%= render :partial => 'form', :locals => {:f => f} %>
      <% end %>
    </div>
  </div>
</div>


