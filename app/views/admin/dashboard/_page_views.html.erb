<div class="table_wrapper">
  <table class="table table-striped table-hover table-sorter">
    <thead>
      <tr>
        <th>User ID</th>
        <th>Email</th>
        <th>Url</th>
        <th>Completed In</th>
        <th>Time</th>
        <th>Status</th>
        <th>XHR</th>
        <th>User Agent</th>
        <th>Controller</th>
        <th>Action</th>
        <th>ID Param</th>
        <th>PID</th>
      </tr>
    </thead>

    <tbody>
      <% records.each do |record| %>
        <tr>
          <td><%= record.user_id %></td>
          <td><%= record.user.email %></td>
          <td><%= record.url %></td>
          <td><%= record.completed_in %></td>
          <td><%= record.created_at.to_s(:datetime_with_seconds) %></td>
          <td><%= record.status %></td>
          <td><%= record.xhr? %></td>
          <td><%= record.user_agent %></td>
          <td><%= record.controller %></td>
          <td><%= record.action %></td>
          <td><%= record.id_parameter %></td>
          <td><%= record.pid %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>

