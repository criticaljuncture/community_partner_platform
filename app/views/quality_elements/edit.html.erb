<%= breadcrumb [['Quality Elements', quality_elements_path], [@quality_element.name, quality_element_path(@quality_element)], ["Edit Quality Element", ""]] %>

<div class="row-fluid">
  <div class="span12">
    <div class="header">
      <h1><%= @quality_element.name %></h1>
    </div>

    <div class="form_wrapper">
      <%= semantic_form_for @quality_element, method: :put, html: {class: "form-horizontal"} do |f| %>
        <%= render :partial => 'form', :locals => {:f => f} %>
      <% end %>
    </div>
  </div>
</div>


