<%= breadcrumb [
  [I18n.t('quality_element.class.name').pluralize, quality_elements_path],
  ["Create", ""]
] %>

<div class="row-fluid">
  <div class="span12">
    <%= page_header("Create New #{I18n.t('quality_element.class.name')}", "") %>

    <div class="form_wrapper">
      <%= simple_form_for @quality_element, method: :post, html: {class: "form-horizontal"} do |f| %>
        <%= render :partial => 'form', :locals => {:f => f} %>
      <% end %>
    </div>
  </div>
</div>
