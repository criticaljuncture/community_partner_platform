<%= breadcrumb [
  ['Community Programs', community_programs_path()],
  [truncate(@community_program.name, length: 20), community_program_path(@community_program)],
  ['Edit', ""]
] %>

<div class="row-fluid">
  <div class="span12">
    <%= page_header("Edit #{@community_program.name}", "icon-handshake") %>

    <div class="form_wrapper">
      <%= semantic_form_for @community_program, method: :put, html: {class: "form-horizontal"} do |f| %>
        <%= render :partial => 'form', :locals => {:f => f} %>
      <% end %>
    </div>

  </div>
</div>
