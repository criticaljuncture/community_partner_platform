<%= breadcrumb [['Partner Organizations', organizations_path], [@organization.name, organization_path(@organization)], ['Edit Organization', ""]] %>

<div class="row-fluid">
  <div class="span12">
    <div class="header">
      <h1><%= @organization.name %></h1>
    </div>

    <div class="form_wrapper">
      <%= semantic_form_for @organization, method: :put, html: {class: "form-horizontal"} do |f| %>
        <%= render :partial => 'form', :locals => {:f => f} %>
      <% end %>
    </div>
  </div>
</div>
