<%= breadcrumb [
  ['Partner Organizations', organizations_path],
  ['Create', ""]
] %>

<div class="row-fluid">
  <div class="span12">
    <%= page_header("Create New Organization", "icon-cpp-small-business") %>

    <div class="form_wrapper">
      <%= semantic_form_for @organization, method: :post, html: {class: "form-horizontal"} do |f| %>
        <%= render :partial => 'form', :locals => {:f => f} %>
      <% end %>
    </div>
  </div>
</div>
